{
  "name": "electron-quick-start",
  "version": "0.0.1",
  "description": "An electron boilerplate with Webpack, TypeScript, React, Redux, and Jest.",
  "main": "main.js",
  "scripts": {
    "test": "jest",
    "test:watch": "npm test -- --watch",
    "dev": "./node_modules/.bin/webpack --config webpack.electron.config.js --mode development",
    "prod": "./node_modules/.bin/webpack --config webpack.electron.config.js --mode production",
    "web:dev": "./node_modules/.bin/webpack --config webpack.config.js --mode development",
    "web:prod": "./node_modules/.bin/webpack --config webpack.config.js --mode production",
    "watch": "./node_modules/.bin/webpack --config webpack.electron.config.js --mode development --watch",
    "live": "./node_modules/.bin/webpack-dev-server --open --mode development --content-base dist-web/ --watch-content-base",
    "lint": "tslint -c tslint.json 'src/**/*.ts*'",
    "check": "echo 'Checking code for errors...' && npm run lint && npm install && npm run prod && npm run test && echo 'Code is in good state. No linting, build, or test errors found.'",
    "prestart": "npm run dev",
    "start": "./node_modules/.bin/electron --inspect=5858 ./dist/main.js",
    "package-with-version": "cp package.json ./dist/package.json && PACKAGE_VERSION=$(cat package.json | grep version | head -1 | awk -F: '{ print $2 }' | sed 's/[\",]//g' | tr -d '[[:space:]]') && ./node_modules/.bin/electron-packager ./dist Neuron\\ $PACKAGE_VERSION --overwrite --out bin/ --icon src/assets/icon.icns",
    "_cp-package-json": "cp package.json ./dist/package.json",
    "_package": "./node_modules/.bin/electron-packager ./dist Neuron --overwrite --out bin/",
    "package": "npm run _cp-package-json && npm run _package"
  },
  "author": "Nono Mart√≠nez Alonso",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^11.12.2",
    "@types/classnames": "^2.2.5",
    "@types/enzyme": "^3.1.14",
    "@types/enzyme-adapter-react-16": "^1.0.2",
    "@types/jest": "^23.3.5",
    "@types/react": "^16.7.17",
    "@types/react-dom": "^16.0.11",
    "@types/react-numeric-input": "^2.2.3",
    "@types/react-redux": "^6.0.9",
    "@types/react-resize-detector": "^3.1.0",
    "@types/redux-logger": "^3.0.6",
    "@types/redux-mock-store": "^1.0.0",
    "awesome-typescript-loader": "^5.2.1",
    "clean-webpack-plugin": "^1.0.0",
    "css-loader": "^2.1.1",
    "dotenv": "^6.1.0",
    "dotenv-webpack": "^1.7.0",
    "electron": "^5.0.1",
    "electron-connect": "^0.6.3",
    "electron-packager": "^13.0.1",
    "electron-reload-webpack-plugin": "^2.0.4",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "fs-extra": "^7.0.0",
    "html-webpack-plugin": "^5.5.1",
    "jest": "^23.6.0",
    "jest-junit": "^5.2.0",
    "mocha-junit-reporter": "^1.18.0",
    "node-sass": "^4.12.0",
    "react-hot-loader": "^4.8.4",
    "react-test-renderer": "^16.5.2",
    "redux-logger": "^3.0.6",
    "redux-mock-store": "^1.5.3",
    "redux-testkit": "^1.0.6",
    "sass-loader": "^7.1.0",
    "selenium-standalone": "^6.15.4",
    "style-loader": "^0.23.1",
    "svg-inline-loader": "^0.8.2",
    "ts-jest": "^23.10.5",
    "ts-loader": "^6.0.0",
    "tslint": "^5.11.0",
    "tslint-loader": "^3.5.4",
    "tslint-react": "^3.5.4",
    "typescript": "^3.2.2",
    "wdio-mocha-framework": "^0.6.2",
    "webdriverio": "^4.14.1",
    "webpack": "^4.31.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "@types/express": "^4.16.1",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-es2015-node": "^6.1.1",
    "babel-preset-react": "^6.24.1",
    "classnames": "^2.2.6",
    "electron-reload": "^1.4.0",
    "fs": "0.0.2",
    "os": "^0.1.1",
    "path": "^0.12.7",
    "prop-types": "^15.6.2",
    "react": "^16.6.3",
    "react-dom": "^16.6.3",
    "react-numeric-input": "^2.2.3",
    "react-redux": "^5.0.7",
    "redux": "^4.0.0",
    "standard": "^12.0.1",
    "standard-loader": "^6.0.1"
  }
}
